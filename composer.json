{
    "name": "wernerdweight/token-generator",
    "type": "library",
    "description": "Simple PHP random token generator",
    "keywords": [
        "random",
        "token",
        "generator"
    ],
    "homepage": "https://github.com/wernerdweight/TokenGenerator",
    "license": "MIT",
    "authors": [
        {
            "name": "Werner Dweight Solutions",
            "email": "info@wds.blue"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "wernerdweight/cs": "^1.1.1"
    },
    "suggest": {},
    "autoload": {
        "psr-0": {
            "WernerDweight\\TokenGenerator\\": ""
        }
    },
    "target-dir": "WernerDweight/TokenGenerator",
    "scripts": {
        "fix": "ecs check ./TokenGenerator.php ./Tests/ --config ecs.yaml --fix",
        "phpstan": "phpstan analyse ./TokenGenerator.php ./Tests/ --level max",
        "phpmd": "phpmd ./TokenGenerator.php text vendor/wernerdweight/cs/WernerDweight/CS/phpmd.xml",
        "ecs": "ecs check ./TokenGenerator.php ./Tests/ --config ecs.yaml",
        "ecs-fix": "ecs check ./TokenGenerator.php ./Tests/ --config ecs.yaml --fix",
        "phpunit": "phpunit",
        "victor": "victor"
    }
}
